
<!-- If can't recognonize the user, link to /register and /login-->
<!-- Else show email and /logout form-->

<header>
  <div id="header"><a href="http://localhost:8080/">My URL Shortener</a></div>
  <br><br>
  <% if (info === undefined) { %>
    Hi there!<br>Click to <a href="http://localhost:8080/register">register</a>!<br>
      Click to <a href="http://localhost:8080/login">log in</a>!
    <% } else { %>
      Hi there, <%= info.email %>!
      <form method="POST" action="/logout">
      <input class="button" type="submit" value="Logout">
  <% } %>
  </form>
</header>
